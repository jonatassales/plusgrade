{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "start": { "dependsOn": ["build"], "cache": false, "persistent": true },
    "dev": { "cache": false, "persistent": true },
    "clean": { "cache": false }
  }
}
